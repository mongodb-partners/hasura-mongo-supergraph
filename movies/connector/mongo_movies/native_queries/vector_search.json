{
    "name": "movieVectorSearch",
    "representation": "function",
    "inputCollection": "embedded_movies",
    "description": "Movies approximate nearest neighbor search",
    "arguments": {
      "vector": {
        "type": {
          "arrayOf": {
            "scalar": "double"
          }
        }
      },
      "gteYear": {
        "type": {
          "scalar": "int"
        }
      },
      "lteYear": {
        "type": {
          "scalar": "int"
        }
      },
      "numCandidates": {
        "type": {
          "scalar": "int"
        }
      },
      "limit": {
        "type": {
          "scalar": "int"
        }
      }
    },
    "resultDocumentType": "MoviesSearch",
    "objectTypes": {
      "MoviesSearch": {
        "fields": {
          "__value": {
            "type": {
              "arrayOf": {
                "object": "MoviesSearchOutput"
              }
            }
          }
        }
      },
      "MoviesSearchOutput": {
        "fields": {
          "title": {
            "type": {
              "scalar": "string"
            }
          },
          "plot": {
            "type": {
              "scalar": "string"
            }
          },
          "year": {
            "type": {
              "scalar": "int"
            }
          },
          "score": {
            "type": {
              "scalar": "double"
            }
          }
        }
      }
    },
    "pipeline": [
      {
        "$vectorSearch": {
          "index": "vector_index", 
          "path": "plot_embedding", 
          "filter": {
            "$and": [
              {
                "year": {
                  "$gte": "{{ gteYear }}", 
                  "$lte": "{{ lteYear }}"
                }
              }
            ]
          }, 
          "queryVector": "{{ vector }}",
         "numCandidates": "{{ numCandidates }}",
         "limit": "{{ limit }}"
        }
      },
      {
        "$project": {
          "_id": 0,
          "title": 1,
          "plot": 1,
          "year": 1,
          "score": {
            "$meta": "vectorSearchScore"
          }
        }
      },
      {
        "$group": {
          "_id": null,
          "__value": {
            "$addToSet": {
              "title": "$title",
              "plot": "$plot",
              "year": "$year",
              "score": "$score"
            }
          }
        }
      },
      {
        "$project": {
          "_id": 0,
          "__value": 1
        }
      }
    ]
  }